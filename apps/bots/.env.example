NODE_ENV="development"

# Pool slot UUID (set by Coolify during slot creation, used to fetch bot config from API)
POOL_SLOT_UUID="your-coolify-service-uuid"

# Backend URL for API calls
BACKEND_URL="http://localhost:3000/api/trpc"

# Bot authentication token (for API calls)
BOT_AUTH_TOKEN="your-bot-auth-token"

# MinIO credentials for S3-compatible storage
MINIO_ENDPOINT="your_minio_endpoint"
MINIO_ACCESS_KEY="your_minio_access_key"
MINIO_SECRET_KEY="your_minio_secret_key"
MINIO_BUCKET_NAME="your_minio_bucket_name"
MINIO_REGION="us-east-1"
